{
  "index_patterns": ["document-*"],
  "priority": 1,
  "template": {
    "settings": {
      "number_of_shards": 1
    },
    "mappings": {
      "properties": {
        "string_field": {
          "type": "text"
        },
        "int_field": {
          "type": "integer"
        }
      }
    }
  }
}
